{
    "name": "react-skeleton",
    "version": "0.1.0",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "build:report": "VITE_APP_REPORT_BUILD_SIZE=true vite build",
        "serve": "vite preview",
        "style-lint": "stylelint \"**/*.scss\"",
        "style-lint-fix": "stylelint \"**/*.scss\" --fix",
        "storybook": "cross-env TS_NODE_PROJECT=.storybook/tsconfig.json start-storybook -p 6006",
        "build-storybook": "cross-env TS_NODE_PROJECT=.storybook/tsconfig.json build-storybook",
        "lint": "eslint --ignore-path .gitignore ./src --ext .ts,.tsx",
        "lint:fix": "npm run lint -- --fix",
        "prettier:check": "prettier --check \"./**/*.{ts,tsx,json}\"",
        "prettier:write": "npm run prettier:check -- --write",
        "types-check": "tsc",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:coverage:open": "opener ./coverage/lcov-report/index.html",
        "test:clear-cache": "jest --clearCache",
        "prepare": "husky install || true"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.4.0",
        "@fortawesome/free-solid-svg-icons": "^6.4.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@mui/lab": "^5.0.0-alpha.110",
        "@mui/material": "^5.11.0",
        "@mui/x-data-grid": "^5.13.1",
        "@sentry/react": "^7.30.0",
        "@sentry/tracing": "^7.30.0",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "firebase": "^9.15.0",
        "formik": "^2.2.9",
        "i18n": "^0.13.4",
        "i18next": "^21.8.14",
        "i18next-browser-languagedetector": "^6.1.4",
        "i18next-http-backend": "^1.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-dropzone": "^14.2.2",
        "react-error-boundary": "^3.1.4",
        "react-i18next": "^11.18.1",
        "@tanstack/react-query": "4.29.5",
        "@tanstack/react-query-devtools": "4.29.6",
        "react-router-dom": "6.11.2",
        "web-vitals": "^3.1.1",
        "yup": "^0.32.9"
    },
    "devDependencies": {
        "@babel/core": "^7.18.0",
        "@babel/preset-env": "^7.18.9",
        "@commitlint/cli": "^17.4.3",
        "@commitlint/config-conventional": "^17.4.3",
        "@storybook/addon-actions": "^6.5.13",
        "@storybook/addon-docs": "^6.5.13",
        "@storybook/addon-essentials": "^6.5.13",
        "@storybook/addon-interactions": "^6.5.13",
        "@storybook/addon-links": "^6.5.13",
        "@storybook/builder-vite": "^0.2.5",
        "@storybook/react": "^6.5.13",
        "@storybook/testing-library": "^0.0.13",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^14.4.3",
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@types/jest": "^29.2.3",
        "@types/node": "^18.0.6",
        "@types/react": "^18.0.15",
        "@types/react-dom": "^18.0.6",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "@vitejs/plugin-react": "^2.2.0",
        "babel-loader": "^8.2.5",
        "babel-plugin-transform-vite-meta-env": "^1.0.3",
        "commitizen": "^4.3.0",
        "cross-env": "^7.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-react": "^7.30.1",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-storybook": "^0.5.12",
        "husky": "^8.0.3",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "jest-watch-typeahead": "^2.2.2",
        "opener": "^1.5.2",
        "prettier": "2.4.1",
        "rollup-plugin-visualizer": "^5.8.3",
        "sass": "^1.53.0",
        "storybook-react-i18next": "^1.1.2",
        "stylelint": "^14.9.1",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-order": "^5.0.0",
        "ts-jest": "^28.0.7",
        "typescript": "^4.9.3",
        "typescript-plugin-css-modules": "^3.4.0",
        "vite": "^3.2.4",
        "vite-tsconfig-paths": "^3.6.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "overrides": {
        "@mdx-js/react": {
            "react": "^18.2.0",
            "react-dom": "^18.2.0"
        },
        "react-element-to-jsx-string": {
            "react": "^18.2.0",
            "react-dom": "^18.2.0"
        },
        "react-inspector": {
            "react": "^18.2.0",
            "react-dom": "^18.2.0"
        }
    }
}
